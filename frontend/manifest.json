{
  "manifest_version": 3,
  "name": "BTTC Assets Tracker",
  "description": "BTTC Assets Tracker",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    }
  },
  "permissions": ["activeTab", "storage"],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}
